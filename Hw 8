class MyIterable:
    def __init__(self, start, end):
        self.start = start
        self.end = end

    def __iter__(self):
        # This is the generator function
        for i in range(self.start, self.end):
            yield i

# Example usage
my_iterable = MyIterable(1, 5)

for value in my_iterable:
    print(value)
